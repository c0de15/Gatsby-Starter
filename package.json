{
  "name": "gatsby-starter-typescript",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Steven Gilliam <steve@steveg.dev>",
  "dependencies": {
    "gatsby": "^5.9.0",
    "gatsby-image": "^2.2.6",
    "gatsby-plugin-manifest": "^2.2.4",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-sass": "^2.1.3",
    "gatsby-plugin-sharp": "^2.14.4",
    "gatsby-plugin-typescript": "^2.1.2",
    "gatsby-source-filesystem": "^2.1.6",
    "gatsby-transformer-sharp": "^2.2.4",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-helmet": "^5.0.8",
    "prettier": "^1.18.2",
    "tsconfig-palmerhq": "^1.1.0",
    "tslint": "^5.18.0",
    "tslint-config-palmerhq": "^1.1.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3"
  },
  "keywords": [
    "gatsby",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run type-check",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css}'",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/c0de15/Gatsby-Starter"
  },
  "bugs": {
    "url": "https://github.com/c0de15/Gatsby-Starter/issues"
  }
}
